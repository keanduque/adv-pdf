                                <!--BEGIN LIST ITEM-->
                                <#list class.items as item>

                                    <#assign show_price_lower_case = record.custrecord_gelmax_cf_showprice?lower_case>

                                    <#assign show_bestseller_lower_case = record.custrecord_gelmax_cf_bestseller?lower_case>
                                    <#assign show_indent_lower_case = record.custrecord_gelmax_cf_indent?lower_case>
