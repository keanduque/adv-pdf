                                    </td>
                                </tr>
                            </table>
                         </#if>

                        <!--END CLASS-->

                        <!--BEGIN IF ITEM-->
                        <#if class.items?has_content>
                            <!--BEGIN TABLE ITEM-->
                            <table width="auto" cellspacing="0" cellpadding="0" style="border-top:1px solid #000000; margin-bottom:3px;">
