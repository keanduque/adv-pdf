    <#assign total_page = 0 >
    <!--Function for Sum-->
    <#function mysum nums...>
        <#local sum = 0>
        <#list nums as num>
            <#local sum += num>
        </#list>
        <#if nums?size != 0>
            <#return sum>
        </#if>
    </#function>
    <!-- START : PDF INDEX PAGE-->
    <pdf>
        <head>
            <link name="NotoSans" type="font" subtype="truetype" src="${nsfont.NotoSans_Regular}" src-bold="${nsfont.NotoSans_Bold}" src-italic="${nsfont.NotoSans_Italic}" src-bolditalic="${nsfont.NotoSans_BoldItalic}" bytes="2" />
            <#if .locale == "zh_CN">
                <link name="NotoSansCJKsc" type="font" subtype="opentype" src="${nsfont.NotoSansCJKsc_Regular}" src-bold="${nsfont.NotoSansCJKsc_Bold}" bytes="2" />
            <#elseif .locale == "zh_TW">
                <link name="NotoSansCJKtc" type="font" subtype="opentype" src="${nsfont.NotoSansCJKtc_Regular}" src-bold="${nsfont.NotoSansCJKtc_Bold}" bytes="2" />
            <#elseif .locale == "ja_JP">
                <link name="NotoSansCJKjp" type="font" subtype="opentype" src="${nsfont.NotoSansCJKjp_Regular}" src-bold="${nsfont.NotoSansCJKjp_Bold}" bytes="2" />
            <#elseif .locale == "ko_KR">
                <link name="NotoSansCJKkr" type="font" subtype="opentype" src="${nsfont.NotoSansCJKkr_Regular}" src-bold="${nsfont.NotoSansCJKkr_Bold}" bytes="2" />
            <#elseif .locale == "th_TH">
                <link name="NotoSansThai" type="font" subtype="opentype" src="${nsfont.NotoSansThai_Regular}" src-bold="${nsfont.NotoSansThai_Bold}" bytes="2" />
            </#if>
            <macrolist>
                <macro id="nlheader"></macro>
                <macro id="nlheader_pasta"></macro>
                <macro id="nlheader_index">
                    <table style="width:100%;" border="1">
                        <tr>
                            <td colspan="80" style="background-color:red" align="center" valign="middle" height="10">
                            <b color="white">INDEX</b>
                            </td>
                        </tr>
                    </table>

                    <table class="catalogue-bg-index" border="0" width="100%" cellspacing="0" cellpadding="0" style="margin-top:22px">
                        <tr><td>
                            <table class="div-index-page" border="1" style="border-bottom:none; border-right:none">
                                <tr>
                                    <th colspan="28" align="center" class="border-right-index" valign="middle">ITEM DESCRIPTION</th>
                                    <th colspan="4"  align="center" class="border-right-index" valign="middle">UOM</th>
                                    <th colspan="4"  align="center" class="border-right-index" valign="middle">PRICE</th>
                                    <th colspan="4"  align="center" style="border-bottom: 1px solid gray;" valign="middle">PAGE</th>
                                </tr>
                                <!--<#assign counter=0>-->

                                <!-- <#list 1..130 as x> -->

                                <!--<#if counter == 65> -->
                                </table>
                            </td>
                            <td><table class="div-index-page" border="1"  style="border-bottom:none; ">
                                <tr>
                                    <th colspan="28" align="center" class="border-right-index" valign="middle">ITEM DESCRIPTION</th>
                                    <th colspan="4"  align="center" class="border-right-index" valign="middle">UOM</th>
                                    <th colspan="4"  align="center" class="border-right-index" valign="middle">PRICE</th>
                                    <th colspan="4"  align="center" style="border-bottom: 1px solid gray;" valign="middle">PAGE</th>
                                </tr>
                                <!--<#assign counter=0>-->

                                <!--</#if>-->
                                    <tr>
                                        <td colspan="28" height="10" class="border-right-index item-name" color="red" valign="middle">&nbsp;</td>
                                        <td colspan="4" class="border-right-index uom" valign="middle">&nbsp;</td>
                                        <td colspan="4" class="border-right-index price" valign="middle">&nbsp;</td>
                                        <td colspan="4" style="border-bottom:1px solid gray;" class="page" valign="middle">&nbsp;</td>
                                    </tr>
                                <!--<#assign counter++>-->

                                <!-- </#list> -->
                            </table>
                        </td></tr>
                    </table>
                </macro>
            </macrolist>
            <style type="text/css">
                * {
                <#if .locale == "zh_CN">
                    font-family: NotoSans, NotoSansCJKsc, sans-serif;
                <#elseif .locale == "zh_TW">
                    font-family: NotoSans, NotoSansCJKtc, sans-serif;
                <#elseif .locale == "ja_JP">
                    font-family: NotoSans, NotoSansCJKjp, sans-serif;
                <#elseif .locale == "ko_KR">
                    font-family: NotoSans, NotoSansCJKkr, sans-serif;
                <#elseif .locale == "th_TH">
                    font-family: NotoSans, NotoSansThai, sans-serif;
                <#else>
                    font-family: NotoSans, sans-serif;
                </#if>
                }
            </style>
            <link type="stylesheet" src="https://5764979.app.netsuite.com/core/media/media.nl?id=7269&amp;c=5764979&amp;h=cfc4ae90c4fdf6727d59&amp;_xt=.css"/>
        </head>
        <body background-color="white"  header="nlheader_index" header-height="3.8%" footer="none" padding="0.18in 0.18in 0.18in 0.18in" size="A4">
            <!-- START: ITEM INDEX PAGE -->
            <#if data?has_content>
                <!-- INDEX is 130 item per page can be listed -->
                <#assign index_page_box_total = 130>
                <#assign item_length = data.item_index?size>
                <#assign index_page = (item_length / index_page_box_total)?ceiling >
                <#assign item_loop_start = 1 >

                <#if item_length lte index_page_box_total>
                    <#assign item_loop_end = item_length >
                <#else>
                    <#assign item_loop_end = index_page_box_total >
                </#if>

                <#list 1..index_page as page>
                    <table border="0" width="100%"  cellspacing="0" cellpadding="0">
                        <tr>
                            <td>
                                <table class="div-index-page" border="0">

                                    <#list item_loop_start..item_loop_end as item>
                                        <#assign array_idx = item - 1 >
                                        <#if data.item_index[array_idx].is_header?has_content>
                                            <tr>
                                                <td colspan="40" class="class-header-index" align="center" valign="middle">${data.item_index[array_idx].class_name?html}</td>
                                            </tr>
                                        </#if>

                                        <#if page - 1 != 0>
                                            <#assign page_condition = page - 1 >
                                        <#else>
                                            <#assign page_condition = 1 >
                                        </#if>

                                        <#if (item - 1) % (index_page_box_total / 2) == 0 && item != 1 && (item - 1) / (index_page_box_total * page_condition) != 1>

                                            </table>
                                            </td>
                                            <td>
                                            <table class="div-index-page" border="0">
                                                <#if data.item_index[array_idx].is_header?has_content>
                                                    <tr>
                                                        <td colspan="40" class="class-header-index" align="center" valign="middle">${data.item_index[array_idx].class_name?html}</td>
                                                    </tr>
                                                </#if>
                                        </#if>

                                        <#if !data.item_index[array_idx].is_header?has_content>
                                            <tr>
                                                <td colspan="28" height="10" class="border-right-sty-body item-name" valign="middle" align="left" style="padding-left: 5px;">
                                                    <#assign item_name = data.item_index[array_idx].description>
                                                    <#assign max_length = 43>
                                                        <#if item_name?length &lt; max_length>
                                                            ${item_name?html}
                                                        <#else>
                                                            ${item_name?substring(0,max_length)?html} ...
                                                        </#if>
                                                </td>
                                                <td colspan="4" class="border-right-sty-body uom" valign="middle" align="center">${data.item_index[array_idx].uom?html}</td>
                                                <td colspan="4" class="border-right-sty-body price" valign="middle" align="center">${data.item_index[array_idx].price}</td>
                                                <td colspan="4" style="border-bottom:none" class="page" valign="middle" align="center">${data.item_index[array_idx].page}</td>
                                            </tr>
                                            <#assign total_page = mysum(data.item_index[array_idx].page)>

                                            <#macro totalPage(total_page)>
                                                ${total_page?number}
                                            </#macro>
                                        </#if>
                                        <#assign item_loop_start = item + 1 >
                                    </#list>
                                </table>
                            </td>
                        </tr>
                    </table>

                    <#if (item_loop_start - 1) + index_page_box_total gte item_length>
                        <#assign item_loop_end = item_length >
                    <#else>
                        <#assign item_loop_end = (index_page_box_total)  + (item_loop_start - 1) >
                    </#if>

                </#list>
            </#if>
            <!-- END: ITEM INDEX PAGE -->
        </body>
    </pdf>
    <!-- END : PDF INDEX PAGE-->

