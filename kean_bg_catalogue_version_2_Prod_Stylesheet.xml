                                    </td>
                                </tr>
                            </table>
                            </#if>
                        </#if>
                        <#if class.class_name?has_content>

                            <#assign cheese_type = class.is_cheese_type>

                            <table class="header" style="width: 100%; margin-bottom:3px" border="0" cellspacing="0" cellpadding="0">
                                <tr>
                                    <td colspan="80"
